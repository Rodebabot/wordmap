tt
# Import Outlook COM library
import win32com.client as win32
from ayx import Alteryx

# Read the incoming data (only one row from Count Records)
df = Alteryx.read("#1")

# Extract the row count value from the dataframe
row_count = df['Record_Count'][0]

# Create Outlook instance
outlook = win32.Dispatch('outlook.application')
mail = outlook.CreateItem(0)

# Fill in email details
mail.To = 'to@example.com'
mail.CC = 'cc@example.com'
mail.Subject = 'Gff'
mail.Body = f'The file contains {row_count} rows.'

# Send email
mail.Send()
